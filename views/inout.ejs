<body>
    <nav class="navbar navbar-dark navbar-expand-md sticky-top py-3" id="mainNav">
        <div class="container"><a class="navbar-brand d-flex align-items-center" href="/"><span class="bs-icon-sm bs-icon-circle bs-icon-primary shadow d-flex justify-content-center align-items-center me-2 bs-icon" style="padding-top: 0px;padding-bottom: 0px;padding-right: 0px;"><i class="far fa-money-bill-alt" style="font-size: 17px;"></i></span><span></span></a><a class="btn btn-primary" role="button" data-bss-hover-animate="pulse" href="/dashbroad/<%=name1%>">Dashboard</a></div>
    </nav>
    
    <div class="hstack justify-content-between" style="margin-left: 55px;margin-top: 47px;">
        <form method="post" action="/dashbroad/<%=name1%>/inout">
            <fieldset>
                <div>
                    <p>Income or Outcome</p><select name="inout">
                        <optgroup label="income or outcome" >
                            <option value="outcome">outcome</option>
                            <option value="income">income</option>
                        </optgroup>
                    </select>
                </div>
                <div >
                    <p>Type of pay</p><select name="type">
                        <optgroup  name="type">
                            <option value="Cash">Cash</option>
                            <option value="Debit">Debit</option>
                        </optgroup>
                    </select>
                </div>
                <div >
                    <p>How much (THB)</p><input type="text" name="thb">
                </div>
                <div>
                    <p>Picture</p><input type="file" name ="pic">
                </div>
            </div>
            <div class="hstack" style="margin-left: 55px;margin-top: 41px;"><button class="btn btn-primary" type="submit">Submit</button><button class="btn btn-primary" type="button" style="margin-left: 29px;">Cancel</button></div>
           
            </fieldset>
      
            
        
        </form>
    </div>
